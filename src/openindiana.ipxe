#!ipxe

set hipster_version 20160421
set base_url http://mods.unff.de/openindiana_netboot_test

:openindiana_menu
menu OpenIndiana
item hipster Hipster 2016.04
item dev151a Dev 151a 
choose ver || goto openindiana_exit

iseq ${ver} hipster && set src http://dlc.openindiana.org/isos/hipster/OI-hipster-text-${hipster_version}.iso ||
iseq ${ver} dev151a && set src http://dlc.openindiana.org/isos/151a/oi-dev-151a-text-x86.iso ||

:boot_openindiana
imgfree
kernel http://mods.unff.de/openindiana_netboot_test/platform/i86pc/kernel/amd64/unix platform/i86pc/kernel/amd64/unix -v -B console=text,install_media=${base_url},install_boot=${base_url}/boot
initrd http://mods.unff.de/openindiana_netboot_test/platform/i86pc/amd64/boot_archive platform/i86pc/amd64/boot_archive
boot

:openindiana_exit
clear menu
exit 0
